#include "main.h"

char *_strncat(char *dest, char *src, int n)
{
  int len;
  int i;
  len = 0;
  while (dest[len]!='\0')
      len++;
  i=0;
  printf("%d",len);
  while (i < n && src[i]!= '\0')
    {
      dest[len] = src[i];
      len++;
      i++;
    }
  dest[i] ='\0';
  return (dest);
}
      
