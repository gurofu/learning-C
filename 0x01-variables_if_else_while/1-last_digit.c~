#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
  int n,ld;
 
 srand(time(0));
 n = rand() - RAND_MAX / 2;
 /* your code goes there */
 ld = n % 10;
  printf("Last digit of %d is %d ",n,ld);
 if(ld>5)
   printf("and if greater than 5");
 else if(ld == 0)
   printf("and is 0");
 else
   printf("and is less than 6 and not 0");

 return (0);
}
